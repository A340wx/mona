include Makefile.inc

all : lib_user hello clockdemo jpegdemo key map mouse noizbg reversidemo shellsvr

lib_user :
	cd userlib; make

hello :
	cd HelloWorld; make

clockdemo :
	cd clock; make

jpegdemo :
	cd jpegls; make

key :
	cd KeyBoardServer; make

map :
	cd MapServer; make

mouse :
	cd MouseServer; make

noizbg :
	cd noiz2bg; make

reversidemo :
	cd Reversi; make

shellsvr :
	cd ShellServer; make

clean :
	(cd userlib; make clean)
	(cd HelloWorld; make clean)
	(cd clock; make clean)
	(cd jpegls; make clean)
	(cd KeyBoardServer; make clean)
	(cd MapServer; make clean)
	(cd MouseServer; make clean)
	(cd noiz2bg; make clean)
	(cd Reversi; make clean)
	(cd ShellServer; make clean)


