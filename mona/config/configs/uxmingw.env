NASM     = nasm
NDISASM  = ndisasm
CC       = $(MINGWPREFIX)gcc
CXX      = $(MINGWPREFIX)g++
LD       = $(MINGWPREFIX)ld
STRIP    = $(MINGWPREFIX)strip
AR       = $(MINGWPREFIX)ar cvru
RANLIB   = $(MINGWPREFIX)ranlib
INSTALL  = install
CFLAGS   = -nostdlib -Wall -fno-builtin -O3 -DMONA -DDEBUG_BUILD -DHOST_MINGW
CXXFLAGS = $(CFLAGS) -fno-exceptions -fno-rtti
NFLAGS   = -I $(MONADIR)/include/ -f win32
OSTYPE   = `uname -sr`

SYMPREFIX = _
USER_START_FUNCTION = _user_start
