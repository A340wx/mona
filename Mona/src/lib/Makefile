all:
	@cd monapi; $(MAKE) $@
	@cd monalibc; $(MAKE) $@
	@cd gui; $(MAKE) $@
	@cd jpegls; $(MAKE) $@

install:
	@cd monapi; $(MAKE) $@
	@cd monalibc; $(MAKE) $@
	@cd gui; $(MAKE) $@
	@cd jpegls; $(MAKE) $@

clean:
	@cd monapi; $(MAKE) $@
	@cd monalibc; $(MAKE) $@
	@cd gui; $(MAKE) $@
	@cd jpegls; $(MAKE) $@
	rm -f ../../lib/*.a

depend:
	@cd monapi; $(MAKE) $@
	@cd monalibc; $(MAKE) $@
	@cd gui; $(MAKE) $@
	@cd jpegls; $(MAKE) $@
