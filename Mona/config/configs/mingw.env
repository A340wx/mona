NASM     = nasm
NDISASM  = ndisasm
CC       = gcc
CXX      = g++
LD       = ld
STRIP    = strip
AR       = ar cvru
RANLIB   = ranlib
INSTALL  = install
CFLAG_DEFS = -DHOST_MINGW
CFLAGS   = -nostdlib -fno-builtin -Wall -O3 -DMONA -DDEBUG_BUILD -U_WIN32 -UWIN32 $(CFLAG_DEFS) 
CXXFLAGS = $(CFLAGS) -fno-exceptions -fno-rtti
NFLAGS   = -I $(MONADIR)/include/ -f win32
OSTYPE   = `uname -sr`
EXEEXT   = .exe
CSCFLAGS = -o

SYMPREFIX = _
USER_START_FUNCTION = _user_start
