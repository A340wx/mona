NASM     = nasm
NDISASM  = ndisasm
CXX      = g++
LD       = ld
STRIP    = strip
AR       = ar cvru
RANLIB   = ranlib
INSTALL  = install
CXXFLAGS = -nostdlib -fno-exceptions -fno-rtti -fno-builtin -Wall -c -g -O3 -DMONA -DDEBUG_BUILD -DHOST_LINUX3
NFLAGS   = -I $(MONADIR)/include/ -DBUILD_ON_LINUX -f elf
EXEEXT   =
MONAELF_LDS = -T $(MONADIR)/env/monaelf.lds
USER_START_FUNCTION = user_start
